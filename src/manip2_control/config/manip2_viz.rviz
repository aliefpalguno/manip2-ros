Visualization Manager:
  Class: ""
  Displays:
    - Class: rviz/RobotModel
      Enabled: true
      Name: RobotModel
    - Class: rviz/TF
      Enabled: true
      Name: TF
  Global Options:
    Fixed Frame: base_link
  Tools:
    - Class: rviz/Interact
    - Class: rviz/MoveCamera
    - Class: rviz/Select
  Views:
    Current:
      Class: rviz/Orbit
      Distance: 1.5
      Focal Point: {X: 0.0, Y: 0.0, Z: 0.5}
